<!DOCTYPE html>
<head>
  <title>Taulukko</title>
  <% include Include/Head.ejs %>
</head>
   <body>
      <div class="addFlex">
         <h1>Taulukko</h1>
      </div>
      <img class="tausta" src="NodeFiles/Files/Huone1.jpg" alt="">
      <div class="FlexMe">
				<div id="flip">
               <h1 id="tul">TIETIX</h1>
               <div id="panel">
                 <% include Include/nav.ejs %>
               </div>
            </div>
            <div class="InfoTaulukko">
               <h1 id="FontH1">Lisää</h1>
               <hr class="line">
               <div id="BoxInsideDivAddData">
                 <form method="post" action="/addData">

                  <p class="inLine">Inventoinkoodi: </p><br>
                    <input class="NumberInputID" type="number" name="invcode" required><br>

                  <p class="inLine">Nimi/Laitteen tuotemerkki: </p><br>
                    <input type="text" name="brand" required><br>

                  <p class="inLine">Laite: </p><br>
                    <select class="OptionForChoise" name="Laite" required>
                      <option disabled selected value>
                      <option value="Mac">Mac</option>
                      <option value="PC">PC</option>
                      <option value="Näyttö">Näyttö</option>
                      <option value="Näytönohjain">Näytönohjain</option>
                      <option value="Näppäimistö">Näppäimistö</option>
                      <option value="Virtapiuha">Virtapiuha</option>
                      <option value="Hiiri">Hiiri</option>
                      <option value="Muistikampa">Muistikampa</option>
                    </select><br>

                    <p class="inLine">Hylly: </p><br>
                      <select class="OptionForChoise" name="Hylly" required>
                        <option disabled selected value>
                        <option value="Hylly 11">Hylly 11</option>
                        <option value="Hylly 12">Hylly 12</option>
                        <option value="Hylly 13">Hylly 13</option>
                        <option value="Hylly 21">Hylly 21</option>
                        <option value="Hylly 22">Hylly 22</option>
                        <option value="Hylly 23">Hylly 23</option>
                        <option value="Hylly 31">Hylly 31</option>
                        <option value="Hylly 32">Hylly 32</option>
                        <option value="Hylly 33">Hylly 33</option>
                        <option value="Hylly 41">Hylly 41</option>
                        <option value="Hylly 42">Hylly 42</option>
                        <option value="Hylly 43">Hylly 43</option>
                        <option value="Hylly 51">Hylly 51</option>
                        <option value="Hylly 52">Hylly 52</option>
                        <option value="Hylly 53">Hylly 53</option>
                        <option value="Hylly 61">Hylly 61</option>
                        <option value="Hylly 62">Hylly 62</option>
                        <option value="Hylly 63">Hylly 63</option>
                        <option value="Hylly 71">Hylly 71</option>
                        <option value="Hylly 72">Hylly 72</option>
                        <option value="Hylly 73">Hylly 73</option>
                        <option value="Hylly 74">Hylly 74</option>
                        <option value="Hylly 75">Hylly 75</option>
                        <option value="Hylly 81">Hylly 81</option>
                        <option value="Hylly 82">Hylly 82</option>
                        <option value="Hylly 83">Hylly 83</option>
                        <option value="Hylly 84">Hylly 84</option>
                        <option value="Hylly 85">Hylly 85</option>
                        <option value="Hylly 91">Hylly 91</option>
                        <option value="Hylly 92">Hylly 92</option>
                        <option value="Hylly 93">Hylly 93</option>
                        <option value="Hylly 94">Hylly 94</option>
                        <option value="Hylly 95">Hylly 95</option>
                        <option value="Kaappi 1">Kaappi 1</option>
                        <option value="Kaappi 2">Kaappi 2</option>
                        <option value="Kaappi 3">Kaappi 3</option>
                        <option value="Kaappi 4">Kaappi 4</option>
                        <option value="Kaappi 5">Kaappi 5</option>
                      </select><br>

                  <p class="inLine">Kunto: </p><br>
                    <select class="OptionForChoise" name="Kunto" required>
                      <option disabled selected value>
                      <option value="Huono">Huono</option>
                      <option value="Perus">Perus</option>
                      <option value="Hyvä">Hyvä</option>
                      <option value="Erinomainen">Erinomainen</option>
                    </select><br>

                  <p class="inLine">Kpl: </p><br>

                  <div class="section">
                    <div class="range-slider">
                      <input class="input-range count1" type="range" value="1" min="1" max="10" name="kpl" >
                      <span class="range-value">1</span>
                    </div>
                    <br>
                      <span>Arvo:</span>
                      <input class="NumberInput count2" type="number" name="price">
                      <p class="inLine"><strong>€</strong></p><br><br>
                  </div>
                      <span>Yhteensä:</span>
                     <input type="text" name="Total" class="TotalValue" value="">
                      <span><strong>€</strong></span><br><br>

                    <input class="Class101" type="submit" value="Submit">

                 </form>
               </div>
            </div>

            <div class="InfoTaulukkoRight">
               <h1 id="FontH1">Data-Info</h1>
               <hr class="line">
              <!--
              <div>

                 </div>
            -->
            </div>

          </div>
